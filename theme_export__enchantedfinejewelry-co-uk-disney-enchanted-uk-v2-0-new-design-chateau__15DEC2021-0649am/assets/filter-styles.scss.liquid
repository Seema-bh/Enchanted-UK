  .mm-collection {
    .mm-collection-breadcrumb {
      .breadcrumb {
        max-width: none;
        width: 100%;
        padding: 0;
      }
    }
    .mm-collection-title {    
      h2 {
        margin: 0;
        color: $primary-color;
      }
      @media(max-width: 767px) {
        margin-bottom: 10px;
      }
    }
    .mm-product-header {    
      p {
        margin: 0;
        font-size: 14px;
        font-family: $NewyorkRegular;
      }

      .mm-sort-content {
        width: 240px;
        select {
          outline: none;
          width: 100%;
          font-family: $NewyorkRegular;
          text-transform: uppercase;
          letter-spacing: 2px;
          font-size: .75em;
          color: #305C8C;
          border: 1px solid $border-color;
          min-height: 34px;
        }
        @media(max-width: 768px) {
          width: 70%;
        }
      }    
      @media(max-width: 767px) {
        text-align: center;
        padding: 0 0 10px 0;
        p {
          margin: 10px 0;
          -webkit-box-ordinal-group: 4;
          -ms-flex-order: 3;
          order: 3;
        }
      }
    }
    @media(max-width:767px){
      .mm-filter-heading {    
        padding-right: 0;
        h4 {
          display: -webkit-box; 
          display: -moz-box;
          display: flex;
          justify-content: space-between;
          margin-bottom: 15px;
          align-items: center;

          .heading-name {
            font-size: 12px;
            font-weight: 400;
            letter-spacing: 1px;
            text-transform: uppercase;
            color: $secondary-color;
          }
          a {
            font-size: 14px;
            font-weight: 400;
            color: $text-color;
            padding: 5px;
            cursor: pointer;

            &:hover {
              color: $primary-color;
              text-decoration: underline;
            }
          }
          .mm-close {
            display: block;
            text-transform: uppercase;
            font-size: 14px;
            cursor: pointer;
          }
        }
      }
      .mm-filter {
        list-style: none;
        padding-right: 0;

        .filter-option-heading,
        .filter-option-heading-lone,
