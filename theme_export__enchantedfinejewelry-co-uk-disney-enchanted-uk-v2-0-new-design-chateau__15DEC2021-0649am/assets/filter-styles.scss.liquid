            text-transform: uppercase;
            margin: 0;
            padding: 15px 0;
            cursor: pointer;
            font-size: 12px;
            letter-spacing: 1px;
            color: $secondary-color;

            .toggle-icon {
              margin: 1px 5px 0 0;
              display: inline-block;
              width: 10px;
              height: 10px;
              position: relative;
              -webkit-transition: 300ms all;
              -o-transition: 300ms all;
              transition: 300ms all;

              &:before {
                width: 10px;
                height: 2px;
                background: $secondary-color;
                content: "";
                position: absolute;
                left: 0;
                top: 4px;
                -webkit-transition: 300ms all;
                -o-transition: 300ms all;
                transition: 300ms all;
              }

              &:after {
                width: 2px;
                height: 10px;
                background: $secondary-color;
                opacity: 0;
                content: "";
                position: absolute;
                left: 4px;
                top: 0;
                -webkit-transition: 300ms all;
                -o-transition: 300ms all;
                transition: 300ms all;
              }
            }
            &.has--closed {
              color: $primary-color;

              .toggle-icon {
                -webkit-transform: rotate(-90deg);
                -ms-transform: rotate(-90deg);
                transform: rotate(-90deg);

                &:before {
                  background: $primary-color;
                }

                &:after {
                  background: $primary-color;
                  opacity: 1;
                }
              }
            }
          }
        }

