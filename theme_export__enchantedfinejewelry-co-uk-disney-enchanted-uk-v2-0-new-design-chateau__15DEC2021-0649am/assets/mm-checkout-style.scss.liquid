      flex-direction: row;	
      justify-content: space-between;	
      
      input[type="search"] {	
        padding: 0 10px;
        min-width: 0;
        height: 42px;
        background: #ECEEF0;
        border: none;
        border-radius: 4px 0 0 4px;
        flex: 1;
      }
      .input-group__btn {
        flex: 0 1 37px;
      }
      .search__submit {
        font-size: 0;
        padding: 0;
        width: 100%;
        height: 42px;	
        background: url("Search_Button.svg") center center no-repeat #ECEEF0;	
        border: none;	
        border-radius: 0 4px 4px 0;	
      }	
    }	
  }	
  
  > ul {	
    > li {	
      padding: 0 1.5em;	
      .also_visit {
        display: flex;
        align-items: center;
        margin: 0;
        color: #305C8C;
        font-size: 13px;
        font-weight: 600;
        text-transform: uppercase;
        padding-top: 24px;
        display:none;
        img{
          width: 90px;
          padding-left: 10px;
        }
        .also_visit_link {
          display: flex;
          align-items: center;
          font-size: 12px;
          color: #305C8C;
        }
      }
      > a {	
        padding: 10px 0;	
        font-size: 13px;	
        font-weight: 600;	
        letter-spacing: 1px;	
        text-transform: uppercase;	
        color: #305C8C;	
        display: inline-block;	
      }	
    }	
  }	
  
  .mobile__submenu {	
    padding: 0 1.5em;	
    display: none;	
    .subsubmenu-wrapper{
      ul{
        display:none;
      }
    }
    h4 {	
      padding-bottom: 12px;	
      margin: .5em 0;	
      border-bottom: 2px solid #ECEEF0;	
      font-size: 13px;	
      font-weight: 600;	
      letter-spacing: 2px;	
      text-transform: uppercase;	
      color: #305C8C;	
    }	
    
    ul {	
      padding-bottom: 1em;	
      
      li {	
        a {	
          font-family: $NewyorkRegular;	
          font-size: .875em;	
          color: #11214A;	
        }	
      }	
    }
    &[data-megamenu="new-arrivals"]{
      h4,.inner_main_link{display:none;}
    }
    .inner_main_link {
      width: 100%;
      text-align: center;
      margin-bottom: 20px;
      margin-top: 14px;
      .inner_link_btn{
        font-size: 12px;
        padding: 8px 20px;
        border: 1px solid #11214A;
      }
    }
  }	
  
  .has-submenu,.has-subsubmenu {	
    display: flex;	
    flex-direction: row-reverse;	
    justify-content: space-between;	
    align-items: center;	
    
    .toggle-icon {	
      display: inline-block;	
      width: 10px;	
      height: 10px;	
      position: relative;	
      -webkit-transition: 300ms all;	
      -o-transition: 300ms all;	
      transition: 300ms all;	
      &:before {	
        width: 10px;	
        height: 2px;	
        background: #305C8C;	
        content: "";	
        position: absolute;	
        left: 0;	
        top: 4px;	
        -webkit-transition: 300ms all;	
        -o-transition: 300ms all;	
        transition: 300ms all;	
      }	
      &:after {	
        width: 2px;	
        height: 10px;	
        background: #305C8C;	
        content: "";	
        position: absolute;	
        left: 4px;	
        top: 0;	
        -webkit-transition: 300ms all;	
        -o-transition: 300ms all;	
        transition: 300ms all;	
      }	
    }	
    
    &.has--closed {	
      > a {	
        color: #DE7C44;	
      }	
      
      .toggle-icon {	
        -webkit-transform: rotate(-90deg);	
        -ms-transform: rotate(-90deg);	
        transform: rotate(-90deg);	
        &:before {	
          background: #DE7C44;	
          opacity: 0;	
        }	
        &:after {	
          background: #DE7C44;          	
        }	
      }	
    }	
  }	
  .inner_main_link {
    .inner_link_btn{
      font-family: "NewYork-Regular";
      color: #11214A;
    }
  }
}	
