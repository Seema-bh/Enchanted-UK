  max-width: 300px;	
  position: fixed;	
  z-index: 9999;	
  overflow: auto;	
  background: #FFFFFF;	
  left: -100%;	
  top: 0;	
  height: 100%;	
  -webkit-transition: all .5s;	
  -o-transition: all .5s;	
  transition: all .5s;	
  padding-bottom: 50px;
  &.show {	
    left: 0;	
  }	
  
  .profile {	
    padding: 10px 1em;	
    display: flex;	
    flex-direction: row;	
    justify-content: flex-start;	
    align-items: center;	
    
    .profile_pic {	
      margin-right: .5em;	
      width: 50px;	
      height: 50px;	
      background: rgba(17,33,74,0.9);	
      border-radius: 50%;	
      font-size: 2em;	
      color: #FFFFFF;	
      display: inline-flex;	
      flex-direction: column;	
      justify-content: center;	
      align-items: center;	
    }	
    
    .profile_text {	
      font-size: 13px;	
      font-weight: 600;	
      letter-spacing: 1px;	
