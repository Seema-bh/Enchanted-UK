/*========================	
Custom Fonts	
========================*/	
@import url('https://fonts.googleapis.com/css2?family=Playfair+Display:wght@400;600&family=Source+Sans+Pro:wght@300;400;600&display=swap');

$NewyorkRegular: "Playfair Display", serif;	
$NewyorkMedium: "Playfair Display", serif;	
$NewyorkBold: "Playfair Display", serif;

.header__container * {box-sizing: border-box;}
body {
  font-family: "Source Sans Pro", sans-serif !important;
}
h1, 
h2, 
h3, 
h4, 
h5, 
h6 {
  font-family: $NewyorkRegular !important;
}

/*========================	
Custom Colors
========================*/
$white: #FFFFFF;
$borderColor: #D8E9FA;
$orderSummaryBackground: #F5F7F8;
$color-border-form: {{ settings.color_text_field_border }};

.hide {
  display: none !important;
}
.mm-checkout {
  .sidebar::after {
    box-shadow: unset;
  }
  
  .main__footer,
  .main .content-box,
  .main .field__input,
  .review-block ~ .review-block,
  .content-box__row ~ .content-box__row {
    border-color: $borderColor;
  }
  .order-summary {
    background: $orderSummaryBackground;
    height: 100%;
    border-radius: 5px;
    padding: 15px 20px;
    &.order-summary--is-collapsed {
      @media (max-width: 1000px) {
        height: 0;
      }
    }
  }
  
  .order-summary__section--product-list::after,
  .sidebar .total-line-table__tbody+.total-line-table__tbody .total-line:first-child th::before, 
  .sidebar .total-line-table__tbody+.total-line-table__tbody .total-line:first-child td::before, 
  .sidebar .total-line-table__tbody+.total-line-table__footer .total-line:first-child th::before, 
  .sidebar .total-line-table__tbody+.total-line-table__footer .total-line:first-child td::before {
    background-color: $white;
  }
  .logo__text.heading-1{
    display: none;
  }
}

.search-header {
  display: inline-block;
  position: relative;
  width: 100%;
  max-width: calc(30em / 16);
  vertical-align: middle;
  
  &.search--focus {
    max-width: 250px;
  }
}

.search-header__input {
  cursor: pointer;
}

.search--focus {
  .search-header__input {
    outline: none;
    border-color: $color-border-form;
    cursor: auto;
  }
  
  .search-header__submit {
    pointer-events: auto;
  }
}

.search-header__submit {
  pointer-events: none;
}

.search-header,
.search-header__submit {
  transition: all 0.35s cubic-bezier(0.29, 0.63, 0.44, 1);
}

.search-bar__submit .icon {
  position: relative;
  top: -1px;
  width: 1.2rem;
  height: auto;
}

.search-bar__submit,
.search-header__submit {
  display: inline-block;
  vertical-align: middle;
  position: absolute;
  right: 0;
  top: 0;
  padding: 0 12px;
  height: 100%;
  z-index: 1;
}

.btn--link .icon {
  vertical-align: middle;
}
.site-header__icon svg {
  margin-right: 3px;
}
.site-header__icon svg {
  height: 23px;
  width: 22px;
}
.icon {
  display: inline-block;
  width: 20px;
  height: 20px;
  vertical-align: middle;
  fill: currentColor;
}
.search-header__submit {
  pointer-events: none;
}
.btn--link {
  background-color: transparent;
  border: 0;
  margin: 0;
  color: #3d4246;
  text-align: left;
}

.search-header__input, .search-bar__input {
  padding-left: 1em;
  background-color: transparent;
  box-sizing: border-box !important;
  border-radius: 2px;
  color: #3d4246;
  border-color: transparent;
  width: 100%;
  min-height: 44px;
}

.visually-hidden, .icon__fallback-text {
  position: absolute !important;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px;
  width: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
}

.dark_overlay {	
  width: 100%;	
  height: 100%;	
  background: rgba(0, 0, 0, 0.6);	
  display: none;	
  position: absolute;	
  left: 0;	
  top: 0;	
  z-index: 999;	
}

.visible-from-1200{
  @media (max-width: 1024px) {	
    display:none !important;
  }
}
.visible-up-tab{
  @media (min-width: 1025px) {	
    display:none !important;
  }
}
