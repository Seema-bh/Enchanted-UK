{%- assign collection = collections[section.settings.collection] -%}
<div class="banner-and-products-section reverse-mobile">
  <div class="left-content">
    <div class="videoWrapper-test">
<!--       <iframe src="https://www.facebook.com/plugins/video.php?href=https%3A%2F%2Fwww.facebook.com%2F1724644634463973%2Fvideos%2F245117470162565%2F&show_text=0&width=267" width="100%" height="700" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allowFullScreen="true"></iframe> -->
      <iframe src="https://www.facebook.com/plugins/video.php?href=https%3A%2F%2Fwww.facebook.com%2Fenchanteddisneyfinejewelry%2Fvideos%2F313660769937025%2F&show_text=0&width=267" width="100%" height="690" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allowFullScreen="true"></iframe>
    </div>
  </div>
  <div class="right-content">
    <div class="product-grid-slider mid-annual-products-box ">
      {% if section.settings.title != blank %}
      <div class="section-header text-center">
        <h2 class="h3">{{ section.settings.title | escape }}</h2>
      </div>
      {% endif %}
      <ul class="grid grid--uniform mid-annual-grid">
        {% for product in collection.products limit: 4 %}
        <li class="grid__item grid__item--{{section.id}} small--one-half medium-up--one-half mid-annual-grid-item">
          {% include 'product-card-grid', max_height: max_height, product: product, show_vendor: section.settings.show_vendor %}
        </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</div>
<div class="banner-and-products-section margin-0">
  <div class="product-grid-slider mid-annual-products-box">
    <ul class="grid grid--uniform mid-annual-grid">
      {% for product in collection.products offset:4 limit: 4 %}
      <li class="grid__item grid__item--{{section.id}} small--one-half medium-up--one-quarter mid-annual-grid-item">
        {% include 'product-card-grid', max_height: max_height, product: product, show_vendor: section.settings.show_vendor %}
      </li>
      {% endfor %}
    </ul>
  </div>
</div>
<div class="banner-and-products-section reverse-mobile">
  <div class="right-content">
    <div class="product-grid-slider mid-annual-products-box">
      <ul class="grid grid--uniform mid-annual-grid">
        {% for product in collection.products offset:8 limit: 4 %}
        <li class="grid__item grid__item--{{section.id}} small--one-half medium-up--one-half mid-annual-grid-item">
          {% include 'product-card-grid', max_height: max_height, product: product, show_vendor: section.settings.show_vendor %}
        </li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="left-content">
   <a href="{{ section.settings.banner_image_link2 }}">
    <img class="lazyload" data-src="{{ section.settings.banner_image2 | img_url: 'master' }}" data-sizes="auto">
    </a>
  </div>
</div>
<div class="banner-and-products-section margin-0">
  <div class="product-grid-slider mid-annual-products-box">
    <ul class="grid grid--uniform mid-annual-grid">
      {% for product in collection.products offset:12 %}
      <li class="grid__item grid__item--{{section.id}} small--one-half medium-up--one-quarter mid-annual-grid-item">
        {% include 'product-card-grid', max_height: max_height, product: product, show_vendor: section.settings.show_vendor %}
      </li>
      {% endfor %}
    </ul>
  </div>
</div>
